						nomeWFS: "decsiraogc_derivazioni:OggettoAssociatoStazPompaggio/decsiraogc_derivazioni:nomeWFS/text()",
						xpath: "decsiraogc_derivazioni:OggettoAssociatoStazPompaggio/decsiraogc_derivazioni:idOggetto/text()"
